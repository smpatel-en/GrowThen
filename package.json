{
  "name": "growthan",
  "version": "1.0.0",
  "description": "Figma to Tailwind CSS multipage website",
  "license": "ISC",
  "author": "Sujal Patel",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tailwindcss -i css/input.css -o css/output.css",
    "dev": "concurrently \"npx live-server\" \"tailwindcss -i css/input.css -o css/output.css --watch\"",
    "watch": "tailwindcss -i css/input.css -o css/output.css --watch",
    "start": "npx live-server"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "live-server": "^1.2.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14"
  }
}
